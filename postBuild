#!/usr/bin/env bash

# Activate kernel
source activate kernel

# Clean datasets
python code/clean_data.py

# Remove raw datasets
GRBS=( 050724 051016B 051227 060614 061006 061210 070714B 071227 080123 080503 100212A 100522A 111121A 150424A 160410A )

for GRB in "${GRBS[@]}"
do
  rm data/SGRBS/GRB_raw.txt
  echo "Deleted: data/SGRBS/GRB_raw.txt"
done
